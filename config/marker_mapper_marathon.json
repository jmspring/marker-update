{
  "id": "mapper",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "jmspring/marker-mapper:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "hostPort": 0, "containerPort": 8080, "servicePort": 10080 }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "MAPBASE_KEY": "pk.eyJ1Ijoiam1zcHJpbmciLCJhIjoiY2luMGl1b2FjMDllZHZsbTRhN3hybWp0eSJ9.KI0FDq_c4jeB2YgmlDw_6Q",
    "PORT": "8080",
    "FETCH_HOST": "marathon-lb-internal.marathon.mesos",
    "FETCH_PORT": "10031",
    "DEPLOY_TIME": "##TIME##"
  },
  "instances": ##INSTANCES##,
  "cpus": 0.25,
  "mem": 256,
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/",
    "portIndex": 0,
    "timeoutSeconds": 10,
    "gracePeriodSeconds": 10,
    "intervalSeconds": 2,
    "maxConsecutiveFailures": 10
  }],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "jimsdecdagents.westus.cloudapp.azure.com",
    "HAPROXY_0_STICKY": "true",
    "HAPROXY_0_BACKEND_HEAD": "\nbackend mapper_10080\n  balance roundrobin\n  mode http\n  reqrep ^([^\\ ]*)\\ /mapper/(.*)     \\1\\ /\\2\n"
  }
}