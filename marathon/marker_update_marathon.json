{
  "id": "update",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "jmspring/marker-update:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "hostPort": 0, "containerPort": 4242, "servicePort": 10042 }
      ],
      "forcePullImage":true
    }
  },
  "env": {
    "MARATHON_URL": "http://172.16.0.5/marathon/v2"
  }
  "instances": 1,
  "cpus": 0.1,
  "mem": 256,
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/",
    "portIndex": 0,
    "timeoutSeconds": 10,
    "gracePeriodSeconds": 10,
    "intervalSeconds": 2,
    "maxConsecutiveFailures": 10
  }],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "jimsdecdagents.westus.cloudapp.azure.com",
    "HAPROXY_0_MODE": "http"
    "HAPROXY_0_PORT": 4242"
  }
}